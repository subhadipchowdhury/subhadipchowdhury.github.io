Cell["\<\
Below we have combined all of the commands from last lab to give a compact \
operation. Run it first.\
\>", "Text",
 CellChangeTimes->{{3.762534242637597*^9, 3.76253427504808*^9}, {
  3.762534814072857*^9, 3.762534823679985*^9}},
 FontFamily->"Palatino",
 FontSize->18,
 FontWeight->"Regular",ExpressionUUID->"1f506398-452c-4b99-a6da-aae4ff3bd07b"],

Cell[BoxData[
 RowBox[{
  RowBox[{"ClassifyStationaryPoints", "[", 
   RowBox[{"f_", ",", 
    RowBox[{"{", 
     RowBox[{"x_", ",", "y_"}], "}"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"X", ",", "P", ",", "H", ",", "g", ",", "d", ",", "S"}], "}"}], 
    ",", 
    RowBox[{
     RowBox[{"X", "=", 
      RowBox[{"{", 
       RowBox[{"x", ",", "y"}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"P", "=", 
      RowBox[{"Solve", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Grad", "[", 
          RowBox[{"f", ",", "X"}], "]"}], "\[Equal]", "0"}], ",", "X", ",", 
        "Reals"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"H", "=", 
      RowBox[{"Grad", "[", 
       RowBox[{
        RowBox[{"Grad", "[", 
         RowBox[{"f", ",", "X"}], "]"}], ",", "X"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"g", "=", 
      RowBox[{"H", "[", 
       RowBox[{"[", 
        RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"d", "=", 
      RowBox[{"Det", "[", "H", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"TableForm", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"{", 
         RowBox[{"x", ",", "y", ",", "d", ",", "g", ",", "f"}], "}"}], "/.", 
        "P"}], ",", 
       RowBox[{"TableHeadings", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"None", ",", 
          RowBox[{"{", 
