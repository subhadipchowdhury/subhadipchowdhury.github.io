Cell[BoxData[
 RowBox[{
  RowBox[{"squaredResidualPlot", "[", 
   RowBox[{"data_List", ",", " ", 
    RowBox[{"int_", "?", "NumberQ"}], ",", " ", "\n", 
    RowBox[{"slope_", "?", "NumberQ"}], ",", " ", "opts___Rule"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "x", ",", "xdata", ",", "ydata", ",", "line", ",", "pred", ",", "resid", 
      ",", "i", ",", "l", ",", "p", ",", "redEdges", ",", "squares", ",", 
      "dashedEdges"}], "}"}], ",", 
    RowBox[{
     RowBox[{"xdata", "=", 
      RowBox[{"data", "[", 
       RowBox[{"[", 
        RowBox[{"All", ",", "1"}], "]"}], "]"}]}], ";", 
     RowBox[{"ydata", "=", 
      RowBox[{"data", "[", 
       RowBox[{"[", 
        RowBox[{"All", ",", "2"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"line", "=", 
      RowBox[{"int", " ", "+", " ", 
       RowBox[{"slope", "*", "x"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"slope", "==", "0"}], ",", 
       RowBox[{"pred", "=", 
        RowBox[{"Table", "[", 
         RowBox[{"int", ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", 
            RowBox[{"Length", "[", "xdata", "]"}]}], "}"}]}], "]"}]}], ",", 
       " ", 
       RowBox[{"pred", "=", 
        RowBox[{"line", " ", "/.", " ", 
         RowBox[{"x", "\[Rule]", "xdata"}]}]}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"resid", "=", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"slope", ">", "0"}], ",", " ", 
        RowBox[{"ydata", "-", "pred"}], ",", " ", 
        RowBox[{"pred", "-", "ydata"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"l", "=", 
      RowBox[{"ListPlot", "[", 
       RowBox[{"data", ",", " ", "opts", ",", 
        RowBox[{"AxesOrigin", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{"0", ",", "0"}], "}"}]}], ",", 
        RowBox[{"PlotRange", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"-", "5"}], ",", "10"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"-", "5"}], ",", "10"}], "}"}]}], "}"}]}], ",", 
        RowBox[{"PlotStyle", "\[Rule]", 
         RowBox[{"AbsolutePointSize", "[", "4", "]"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"p", "=", 
      RowBox[{"Plot", "[", 
       RowBox[{"line", ",", " ", 
        RowBox[{"{", 
         RowBox[{"x", ",", "0", ",", "7"}], "}"}], ",", "opts", ",", 
        RowBox[{"PlotStyle", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{"Hue", "[", ".7", "]"}], "}"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"redEdges", "=", 
      RowBox[{"Prepend", "[", 
       RowBox[{
        RowBox[{"MapThread", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Line", "[", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"#1", ",", "#3"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"#1", "-", "#4"}], ",", "#3"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"#1", "-", "#4"}], ",", "#2"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"#1", ",", "#2"}], "}"}]}], "}"}], "]"}], "&"}], ",", 
          RowBox[{"{", 
           RowBox[{"xdata", ",", "ydata", ",", "pred", ",", "resid"}], 
           "}"}]}], "]"}], ",", 
        RowBox[{"Hue", "[", "1", "]"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"squares", "=", 
      RowBox[{"Prepend", "[", 
       RowBox[{
        RowBox[{"MapThread", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Rectangle", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"#1", "-", "#4"}], ",", "#3"}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{"#1", ",", "#2"}], "}"}]}], "]"}], "&"}], ",", 
          RowBox[{"{", 
           RowBox[{"xdata", ",", "ydata", ",", "pred", ",", "resid"}], 
           "}"}]}], "]"}], ",", 
        RowBox[{"Opacity", "[", ".2", "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"dashedEdges", "=", 
      RowBox[{"Prepend", "[", 
       RowBox[{
        RowBox[{"MapThread", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Line", "[", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"#1", ",", "#2"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"#1", ",", "#3"}], "}"}]}], "}"}], "]"}], "&"}], ",", 
          RowBox[{"{", 
           RowBox[{"xdata", ",", "ydata", ",", "pred"}], "}"}]}], "]"}], ",", 
        RowBox[{"Dashing", "[", 
         RowBox[{"{", ".01", "}"}], "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Show", "[", 
      RowBox[{"l", ",", "p", ",", 
       RowBox[{"Graphics", "[", "squares", "]"}], ",", 
       RowBox[{"Graphics", "[", "redEdges", "]"}], ",", 
       RowBox[{"Graphics", "[", "dashedEdges", "]"}], ",", 
       RowBox[{"AspectRatio", "\[Rule]", "Automatic"}], ",", 
       RowBox[{"AxesLabel", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"\"\<x\>\"", ",", " ", "\"\<y\>\""}], "}"}]}], ",", 
       RowBox[{"PlotLabel", "\[Rule]", 
        RowBox[{"\"\<sum of squares: \>\"", "<>", 
         RowBox[{"ToString", "[", 
          RowBox[{"PaddedForm", "[", 
           RowBox[{
            RowBox[{"Plus", "@@", 
             RowBox[{"(", 
              SuperscriptBox["resid", "2"], ")"}]}], ",", 
            RowBox[{"{", 
             RowBox[{"6", ",", "3"}], "}"}]}], "]"}], "]"}]}]}]}], "]"}]}]}], 
   "\n", "]"}]}]], "Input",
