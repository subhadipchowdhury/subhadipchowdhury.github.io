Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"DataAdjacencyPlot", "::", "usage"}], "=", 
   "\"\<DataAdjacencyPlot[data,adj] takes a two-row matrix data whose columns \
are the x,y coordinates of points in the plane and an adjacency matrix adj \
telling which of the points in data are to be connected. The result is the \
plot of the data connected as specified. (The matrix data can also be a \
three-row matrix whose columns are homogeneous coordinates with 1 as the \
third coordinate.) ListPlot options are accepted.\>\""}], ";"}]], "Input",
 CellOpen->False,
 InitializationCell->True,
 CellChangeTimes->{{3.4341191318745604`*^9, 3.434119327248255*^9}, {
  3.434146315898328*^9, 3.434146333834453*^9}, {3.4341481278119993`*^9, 
  3.4341481401912003`*^9}, {3.434299989735886*^9, 3.434300088013722*^9}, {
