4f2d0cddf9ca"],

Cell[BoxData[
 RowBox[{
  RowBox[{"DataAdjacencyPlot3D", "[", 
   RowBox[{"data3D_", ",", "adj_", ",", "cop_", ",", "opts___Rule"}], "]"}], ":=",
   "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "b", ",", "c", ",", "d", ",", "dd0", ",", "dd1", ",", "dd2", ",", "dd4", 
      ",", "ddd"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"b", ",", "c", ",", "d"}], "}"}], "=", "cop"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"dd0", ":=", 
      RowBox[{
       RowBox[{"PerspectiveProjection", "[", 
        RowBox[{"b", ",", "c", ",", "d"}], "]"}], ".", "data3D"}]}], ";", 
     "\n", "     ", 
     RowBox[{"dd1", ":=", 
      RowBox[{"dd0", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"dd2", ":=", 
      RowBox[{"dd0", "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"dd4", ":=", 
      RowBox[{"dd0", "[", 
       RowBox[{"[", "4", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"ddd", ":=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"dd1", "/", "dd4"}], ",", 
        RowBox[{"dd2", "/", "dd4"}]}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"DataAdjacencyPlot", "[", 
      RowBox[{"ddd", ",", "adj", ",", "\[IndentingNewLine]", 
       RowBox[{"Evaluate", "[", 
        RowBox[{"Sequence", "@@", 
         RowBox[{"FilterRules", "[", 
          RowBox[{
           RowBox[{"{", "opts", "}"}], ",", 
           RowBox[{"Options", "@", "ListPlot"}]}], "]"}]}], "]"}]}], 
      "]"}]}]}], "]"}]}]], "Input",
