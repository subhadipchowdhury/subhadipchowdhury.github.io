Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"DataAdjacencyPlot3D", "::", "usage"}], "=", 
   "\"\<DataAdjacencyPlot3D[data,adj,{b,c,d}] \\\naccepts a matrix data whose \
columns consist of homogeneous coordinates \\\nfor points in space (with \
fourth coordinate equal to 1), an adjacency \\\nmatrix adj telling which of \
the points in data are to be connected, and \\\na center of projection \
{b,c,d}. The result is the perspective plot of the \\\ndata connected as \
specified. ListPlot options are accepted.\>\""}], ";"}]], "Input",
 CellOpen->False,
 InitializationCell->True,
 CellChangeTimes->{{3.434150377206883*^9, 3.434150571058138*^9}, {
