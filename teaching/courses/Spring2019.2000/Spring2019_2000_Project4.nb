0b72c9be3e14"],

Cell[BoxData[
 RowBox[{
  RowBox[{"DataAdjacencyPlot", "[", 
   RowBox[{"data_", ",", "adj_", ",", "opts___Rule"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "ddata", ",", "adjacency", ",", "datat", ",", "ndata", ",", "datalines", 
      ",", "i", ",", "j"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", "data", "]"}], "\[Equal]", "2"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"ddata", "=", "data"}], ",", "\[IndentingNewLine]", 
       RowBox[{"ddata", "=", 
        RowBox[{"data", "[", 
         RowBox[{"[", 
          RowBox[{
           RowBox[{"1", ";;", "2"}], ",", "All"}], "]"}], "]"}]}]}], "]"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"adjacency", "=", "adj"}], ";", "\[IndentingNewLine]", 
     RowBox[{"datat", "=", 
      RowBox[{"Transpose", "[", "ddata", "]"}]}], ";", "\n", "    ", 
     RowBox[{"ndata", "=", 
      RowBox[{"Length", "[", "datat", "]"}]}], ";", "\n", "    ", 
     RowBox[{"datalines", "=", "\[IndentingNewLine]", 
      RowBox[{"Map", "[", 
       RowBox[{"Line", ",", 
        RowBox[{"Select", "[", 
         RowBox[{
          RowBox[{"Flatten", "[", 
           RowBox[{
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"adjacency", "[", 
                  RowBox[{"[", 
                   RowBox[{"i", ",", "j"}], "]"}], "]"}], "\[Equal]", "1"}], 
                ",", 
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"datat", "[", 
                   RowBox[{"[", "i", "]"}], "]"}], ",", 
                  RowBox[{"datat", "[", 
                   RowBox[{"[", "j", "]"}], "]"}]}], "}"}]}], "]"}], ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", "1", ",", 
                RowBox[{"ndata", "-", "1"}]}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"j", ",", 
                RowBox[{"i", "+", "1"}], ",", "ndata"}], "}"}]}], "]"}], ",", 
            "1"}], "]"}], ",", 
          RowBox[{
           RowBox[{"#", "=!=", "Null"}], "&"}]}], "]"}], ",", 
        RowBox[{"{", "1", "}"}]}], "]"}]}], ";", "\n", "    ", 
     RowBox[{"ListPlot", "[", 
      RowBox[{"datat", ",", "\[IndentingNewLine]", 
       RowBox[{"Epilog", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Thickness", "[", ".005", "]"}], ",", "Blue", ",", " ", 
          "datalines"}], "}"}]}], ",", " ", "\[IndentingNewLine]", 
       RowBox[{"Evaluate", "[", 
        RowBox[{"Sequence", "@@", 
         RowBox[{"FilterRules", "[", 
          RowBox[{
           RowBox[{"{", "opts", "}"}], ",", 
           RowBox[{"Options", "@", "ListPlot"}]}], "]"}]}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"AspectRatio", "\[Rule]", "Automatic"}], ",", 
       RowBox[{"PlotStyle", "\[Rule]", 
        RowBox[{"PointSize", "[", ".001", "]"}]}], ",", 
       RowBox[{"PlotRange", "\[Rule]", "All"}]}], "]"}]}]}], "]"}]}]], "Input",\

