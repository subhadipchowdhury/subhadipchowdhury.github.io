                    $CellContext`F[$CellContext`xx[$CellContext`k - 
                    1] + $CellContext`a $CellContext`t, \
$CellContext`yy[$CellContext`k - 
                    1] + $CellContext`b $CellContext`t]]], {$CellContext`t, 0,
                    1}]] + $CellContext`b Chop[
                 NIntegrate[
                  Simplify[
                   Expand[
                    $CellContext`G[$CellContext`xx[$CellContext`k - 
                    1] + $CellContext`a $CellContext`t, \
$CellContext`yy[$CellContext`k - 
                    1] + $CellContext`b $CellContext`t]]], {$CellContext`t, 0,
                    1}]], {$CellContext`k, $CellContext`n}]]], 
         And[Head[$CellContext`x] == Symbol, Head[$CellContext`y] == Symbol, 
          NumberQ[
           N[
            
            ReplaceAll[$CellContext`Fxy, {$CellContext`x -> 
              Part[$CellContext`PointList, 1, 1], $CellContext`y -> 
              Part[$CellContext`PointList, 1, 2]}]]], 
          NumberQ[
           N[
            
            ReplaceAll[$CellContext`Gxy, {$CellContext`x -> 
              Part[$CellContext`PointList, 1, 1], $CellContext`y -> 
              Part[$CellContext`PointList, 1, 2]}]]]]]; Null),
