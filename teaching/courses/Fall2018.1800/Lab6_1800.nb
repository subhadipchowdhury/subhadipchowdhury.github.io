Cell[TextData[{
 StyleBox["In this lab, we' ll explore properties of four different vector \
fields.\n\n",
  FontFamily->"Palatino",
  FontWeight->"Regular"],
 Cell[BoxData[
  StyleBox[
   RowBox[{
    StyleBox["Press",
     FontWeight->"Bold",
     FontColor->RGBColor[1, 0, 1]], " ", 
    ButtonBox["\<\"Start\"\>",
     Appearance->Automatic,
     ButtonFunction:>(
      Clear[$CellContext`NLineIntegrate]; $CellContext`NLineIntegrate[{
          Pattern[$CellContext`Fxy, 
           Blank[]], 
          Pattern[$CellContext`Gxy, 
           Blank[]]}, {
          Pattern[$CellContext`x, 
           Blank[]], 
          Pattern[$CellContext`y, 
           Blank[]]}, 
         Pattern[$CellContext`PointList, {
           RepeatedNull[{
             Blank[], 
             Blank[]}]}], 
         Pattern[$CellContext`opts, 
          BlankNullSequence[Rule]]] := Condition[
         Module[{$CellContext`initialPoint, $CellContext`terminalPoint, \
$CellContext`pointList = $CellContext`PointList, $CellContext`n, \
$CellContext`xx, $CellContext`yy, $CellContext`k, $CellContext`a, \
$CellContext`b, $CellContext`F, $CellContext`G, $CellContext`s, \
$CellContext`r, $CellContext`t}, $CellContext`initialPoint = ReplaceAll[
             ReplaceAll[$CellContext`InitialPoint, {$CellContext`opts}], 
             Options[$CellContext`LineIntegrate]]; $CellContext`terminalPoint = 
           ReplaceAll[
             ReplaceAll[$CellContext`TerminalPoint, {$CellContext`opts}], 
             Options[$CellContext`LineIntegrate]]; 
          If[$CellContext`initialPoint =!= Automatic, 
            PrependTo[$CellContext`pointList, $CellContext`initialPoint]; 
            Null]; If[$CellContext`terminalPoint =!= Automatic, 
            AppendTo[$CellContext`pointList, $CellContext`terminalPoint]; 
            Null]; $CellContext`F[
             Pattern[$CellContext`s, 
              Blank[]], 
             Pattern[$CellContext`r, 
              Blank[]]] := 
           ReplaceAll[$CellContext`Fxy, {$CellContext`x -> $CellContext`s, \
$CellContext`y -> $CellContext`r}]; $CellContext`G[
             Pattern[$CellContext`s, 
              Blank[]], 
             Pattern[$CellContext`r, 
              Blank[]]] := 
           ReplaceAll[$CellContext`Gxy, {$CellContext`x -> $CellContext`s, \
$CellContext`y -> $CellContext`r}]; $CellContext`n = 
           Length[$CellContext`pointList] - 1; Do[{
              $CellContext`xx[$CellContext`k], 
              $CellContext`yy[$CellContext`k]} = 
            Part[$CellContext`pointList, $CellContext`k + 1], {$CellContext`k,
              0, $CellContext`n}]; Chop[
            
            Sum[$CellContext`a = $CellContext`xx[$CellContext`k] - \
$CellContext`xx[$CellContext`k - 
                1]; $CellContext`b = $CellContext`yy[$CellContext`k] - \
$CellContext`yy[$CellContext`k - 1]; $CellContext`a Chop[
                 NIntegrate[
                  Simplify[
