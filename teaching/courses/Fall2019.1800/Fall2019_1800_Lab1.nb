Cell["\<\
defaultvp={2.8,.3,1.8};

Magnitude[a_]:= Sqrt[Dot[a,a]];

Clear[myvector];
myvector[a_,ccc___]:=Module[{b,c,ii,tt,w,sc,aa},
\tii={-10.112,-10.012,.112};
\ttt=.5;
\tw=Cross[Cross[ii,a],a];
\tw=w/Sqrt[Dot[w,w]];
\taa=Sqrt[Dot[a,a]];
\tsc=Min[.5,aa/4];
\tb=sc(-Cos[tt] a/aa + Sin[tt] w);
\tc=sc(-Cos[tt] a/aa - Sin[tt] w);
\t{Thickness[.02],ccc,Line[{{0,0,0},a,a+b,a,a+c}]}
\t];
\t
myvectordash[a_]:=Module[{b,c,ii,tt,w,sc,aa},
\tii={-10.112,-10.012,.112};
\ttt=.5;
\tw=Cross[Cross[ii,a],a];
\tw=w/Sqrt[Dot[w,w]];
\taa=Sqrt[Dot[a,a]];
\tsc=Max[aa/6,Min[.5,aa/4]];
\tb=sc(-Cos[tt] a/aa + Sin[tt] w);
\tc=sc(-Cos[tt] a/aa - Sin[tt] w);
\t{{Thickness[.02],Dashing[{.02,.02}],
\tLine[{{0,0,0},a}]},
\t {Thickness[.02],Line[{a+b,a,a+c}]}}
\t];
col[i_]:=If[i<=4,
\t{RGBColor[1,0,0],
\tRGBColor[0,1,0],
\tRGBColor[0,0,1],
\tPurple}[[i]],
\tRGBColor[Random[],Random[],Random[]]
\t];
\t
mylabels={\"x\",\"y\",\"z\"};

SeeVector[a_]:=
\tModule[{d},
\td=Floor[Max[Abs[a]]]+1;
\tShow[
\tGraphics3D[{myvector[a], threeaxes[d]},
\tPlotRange->{{-d,d},{-d,d},{-d,d}},
\tAxes->True,
\tAxesLabel->mylabels,
\tViewPoint->defaultvp
\t\t]
\t]];\t
\t
Clear[SeeVectors];
SeeVectors[lii___] :=
  \tModule[{d, li, i},
   \tli = {lii};
\td=Max[3,Ceiling[Max[Abs[li]]]];
   \tShow[
    \tGraphics3D[{threeaxes[d],Table[myvector[li[[i]], col[i]],
      \t\t{i, Length[li], 1, -1}]},
     \tPlotRange -> {{-d, d}, {-d, d}, {-d, d}},
     \tAxes -> True,
     \tAxesLabel -> {\"x\", \"y\", \"z\"},
     \tViewPoint -> defaultvp,
     PlotLabel \[Rule] If[Length[li] \[Equal] 1, li[[1]]]
     \t\t]
    \t]];\t


SeeVectorResult[li_]:=
\tModule[{d,nn},
\tnn=Length[li];
\td=Floor[Max[Abs[li]]]+1;
\tShow[
\tGraphics3D[
\t\tAppend[Map[myvector,Take[li,nn-1]],
\t\t\t\t\tmyvectordash[li[[nn]]] ],
\tPlotRange->{{-d,d},{-d,d},{-d,d}},
\tAxes->True,
\tAxesLabel->mylabels,
\tViewPoint->defaultvp
\t\t]
\t]];
\t\t
SeeVector::usage = \"SeeVector plots one single vector
in three dimensions. The syntax is
SeeVector[x], where x is a three dimensional vector.
For example, SeeVector[{1,2,3}].\";

SeeVectors::usage = \"SeeVectors plots a list of vectors
in three dimensions. The syntax is
SeeVectors[vector_list], where vector_list is a LIST of
three dimensional vectors.
For example, SeeVectors[{a, b, c}], where each of a, b, c 
has been defined as an individual vector.\";

SeeVectorResult::usage = \"SeeVectorResult is the
same as SeeVectors except that the last vector in
the list will be highlighted as a dashed vector.\";

SpinAround[gr_]:=SpinAroundHeight[gr,defaultvp[[3]]];

SpinAroundHeight[gr_,height_]:= Module[{len,tt,t},
\ttt=16;
\tlen=Magnitude[Take[defaultvp,2]];
\tTable[Show[gr,ViewPoint->
\t\t\t{len Cos[2 Pi t/tt +.15], 
\t\t\tlen Sin[2 Pi t/tt +.15],
\t\t\t\theight},
\t\t\t\tViewCenter->{1/2,1/2,1/2} ],
\t\t{t,1,tt}];
\t];
\t\t
mysign[vv_,kk_]:= Abs[(Sign[vv]+3)/2-kk]+1

IWantAxes:= (mewannaaxes=1;
\t\tPrint[\"Axes option turned on.\"];);
IDontWantAxes:= (mewannaaxes=0;
\t\tPrint[\"Axes option turned off.\"];);
mewannaaxes=1;

threeaxes[d_] :=
\tIf[mewannaaxes==1,
\t{{Thickness[.002],Line[{{-d,0,0},{d,0,0}}]},
\t {Thickness[.002],Line[{{0,-d,0},{0,d,0}}]},
\t {Thickness[.002],Line[{{0,0,-d},{0,0,d}}]}
\t},
\t{{Line[{{1000,0,0},{1000,0,0}}]}}];

drawline[r0_,v_,dimen_] :=
\tModule[{tt,t,aa,k,kk,ourr},
\ttt={{},{}};
\taa=r0 + t v;
\tDo[If[v[[k]] !=0,
\t\t\tourr=Solve[aa[[k]]==dimen[[k,kk]],t];
\t\t\tAppendTo[tt[[mysign[v[[k]],kk]]],t /. ourr]
\t\t],
\t{k,1,3},{kk,1,2}];
\t{Thickness[.02],
\tLine[{r0+Min[tt[[1]]]v,r0+Max[tt[[2]]]v}]}
\t];


PlotLine[r0_,v_]:=
\tModule[{d,dimen},
\td=Floor[Max[Abs[{r0,r0+v,r0-v,v}]]]+1;
\tdimen={{-d,d},{-d,d},{-d,d}};
\tShow[
\t\tGraphics3D[drawline[r0,v,dimen],
\tPlotRange->dimen,
\tAxes->True,
\tAxesLabel->mylabels,
\tViewPoint->defaultvp
\t\t]
\t]];

PlotLine::usage = \"PlotLine plots one single line
in three dimensions. The syntax is
PlotLine[r0, v], where r0, v are the vectors
for the parametric equation of a line
in the form r = r0 + t v.
For example, PlotLine[{1,2,3}, {-2,0,1}].\";

PlotLines[li_]:=
\tModule[{rr,vv,d,dimen,f},
\trr=Transpose[li][[1]];
\tvv=Transpose[li][[2]];
\td=Floor[Max[Abs[{rr,rr+vv,rr-vv,vv}]]]+1;
\tdimen={{-d,d},{-d,d},{-d,d}};
\tf[x_]:=drawline[x[[1]],x[[2]],dimen];
\tShow[
\t\tGraphics3D[{Map[f,li],threeaxes[d]},
\tPlotRange->dimen,
\tAxes->True,
\tAxesLabel->mylabels,
\tViewPoint->defaultvp
\t\t]
\t]];

PlotLineWithVectors[r0_,v_]:=
\tModule[{d,dimen},
\td=Floor[Max[Abs[{r0,r0+v,r0-v,v}]]]+1;
\tdimen={{-d,d},{-d,d},{-d,d}};
\tShow[
\t\tGraphics3D[{drawline[r0,v,dimen],
\t\t\t\tmyvector[r0],
\t\t\t\tmyvector[v],
\t\t\t\tthreeaxes[d]
\t\t\t   },
\tPlotRange->dimen,
\tAxes->True,
\tAxesLabel->mylabels,
\tViewPoint->defaultvp
\t\t]
\t]];\t
drawplane[r_,n_,d_,g_]:=
Module[{ourr,x,y,z},
ourr=Solve[({x,y,z}-r) . n ==0,z];
z = z/. Flatten[ourr];
Plot3D[z,{x,-d,d},{y,-d,d},
\tBoxRatios->{1,1,1},
\tPlotRange->{{-d,d},{-d,d},{-d,d}},
\tClippingStyle->None,
\tPlotPoints->15,
\tMeshStyle->GrayLevel[g],
\tDisplayFunction->Identity]
\t]

PlotPlane::usage = \"PlotPlane plots one single plane
in three dimensions. The syntax is
PlotLine[r0, n], where r0, n are the vectors
for the equation of a plane
in the form (r - r0) . n = 0.
For example, PlotPlane[{1,2,3}, {-2,0,1}].\";

PlotPlane[r_,n_]:=
\tModule[{d,dimen},
\tPrint[r,n];
\td=Floor[Max[Abs[{r,r+n,r-n,n}]]]+1;
\tdimen={{-d,d},{-d,d},{-d,d}};
\tShow[drawplane[r,n,d,.5],
\t\tGraphics3D[{
\t\t\t\tmyvector[r],
\t\t\t\tmyvector[n],
\t\t\t\tthreeaxes[d]
\t\t\t   }],
\tPlotRange->dimen,
\tAxes->True,
\tAxesLabel->mylabels,
\tViewPoint->defaultvp,
\tDisplayFunction->$DisplayFunction
\t\t]
\t];\t
PlotPlanes[li_]:=
\tModule[{rr,vv,nn,d,dimen,f},
\tnn=Length[li];
\trr=Transpose[li][[1]];
\tvv=Transpose[li][[2]];
\td=Floor[Max[Abs[{rr,rr+vv,rr-vv,vv}]]]+1;
\tdimen={{-d,d},{-d,d},{-d,d}};
\tShow[Table[drawplane[li[[k,1]],li[[k,2]],d,
\t\t\t\t\t.4+.1 k],{k,1,nn} ],
\t\tGraphics3D[{Map[myvector,vv],
\t\t\tthreeaxes[d]}],
\tPlotRange->dimen,
\tAxes->True,
\tAxesLabel->mylabels,
\tViewPoint->defaultvp,
\tDisplayFunction->$DisplayFunction
\t\t]
\t];
\t
PlotLineAndPlane[lin_,pla_]:=
\tModule[{rr,vv,nn,d,dimen,f},
\td=Floor[Max[Abs[{lin[[1]],lin[[2]],lin[[1]]+lin[[2]],
\t\t\t\t\tlin[[1]]-lin[[2]],
\t\t\t\t\tpla[[1]],pla[[2]],pla[[1]]+pla[[2]],
\t\t\t\t\tpla[[1]]-pla[[2]]}]]]+1;
\tdimen={{-d,d},{-d,d},{-d,d}};
\tShow[drawplane[pla[[1]],pla[[2]],d,.5],
\t\tGraphics3D[{drawline[lin[[1]],lin[[2]],dimen],
\t\t\t\tthreeaxes[d]}],
\tPlotRange->dimen,
\tAxes->True,
\tAxesLabel->mylabels,
\tViewPoint->defaultvp,
\tDisplayFunction->$DisplayFunction
\t\t]
\t];

