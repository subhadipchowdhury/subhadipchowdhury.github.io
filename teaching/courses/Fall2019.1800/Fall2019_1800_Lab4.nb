Cell[BoxData[
 RowBox[{
  RowBox[{"TangentPlane", "[", 
   RowBox[{"func_", ",", "pt_"}], "]"}], ":=", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"temp", "=", "func"}], ";", 
    RowBox[{"temp1", "=", 
     RowBox[{"(", 
      RowBox[{"func", "/.", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "\[Rule]", 
          RowBox[{"pt", "[", 
           RowBox[{"[", "1", "]"}], "]"}]}], ",", 
         RowBox[{"y", "\[Rule]", 
          RowBox[{"pt", "[", 
           RowBox[{"[", "2", "]"}], "]"}]}]}], "}"}]}], ")"}]}], ";", 
    RowBox[{"temp2", "=", 
     RowBox[{"Grad2DPt", "[", 
      RowBox[{"temp", ",", "pt"}], "]"}]}], ";", 
    RowBox[{"temp1", "+", 
     RowBox[{
      RowBox[{"temp2", "[", 
       RowBox[{"[", "1", "]"}], "]"}], "*", 
      RowBox[{"(", 
       RowBox[{"x", "-", 
        RowBox[{"pt", "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], ")"}]}], "+", 
     RowBox[{
      RowBox[{"temp2", "[", 
       RowBox[{"[", "2", "]"}], "]"}], "*", 
      RowBox[{"(", 
       RowBox[{"y", "-", 
        RowBox[{"pt", "[", 
         RowBox[{"[", "2", "]"}], "]"}]}], ")"}]}]}]}], ")"}]}]], "Input",
 CellChangeTimes->{{3.715463118754326*^9, 3.715463175507228*^9}, {
