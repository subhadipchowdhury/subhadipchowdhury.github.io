Cell["\<\
Following this logic, let\[CloseCurlyQuote]s define a directional derivative \
command, that takes in a specific function, a specific point, and a specific \
direction to head out from that point, and outputs the slope in that \
direction!\
\>", "Text",
 CellChangeTimes->{{3.715467275558715*^9, 3.715467319908121*^9}},
 FontFamily->"Palatino",
 FontSize->18,
 FontWeight->"Regular",
 FontSlant->"Italic",ExpressionUUID->"094b858e-bac2-462c-90de-f9ae133f90c8"],

Cell[BoxData[
 RowBox[{
  RowBox[{"DirecDeriv", "[", 
   RowBox[{"func_", ",", "pt_", ",", "dir_"}], "]"}], ":=", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"temp", "=", 
     RowBox[{"TangentPlane", "[", 
      RowBox[{"func", ",", "pt"}], "]"}]}], ";", 
    RowBox[{"ztip", "=", 
     RowBox[{"temp", "/.", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"x", "\[Rule]", 
         RowBox[{
          RowBox[{"pt", "[", 
           RowBox[{"[", "1", "]"}], "]"}], "+", 
          RowBox[{"dir", "[", 
           RowBox[{"[", "1", "]"}], "]"}]}]}], ",", 
        RowBox[{"y", "\[Rule]", 
         RowBox[{
          RowBox[{"pt", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "+", 
          RowBox[{"dir", "[", 
           RowBox[{"[", "2", "]"}], "]"}]}]}]}], "}"}]}]}], ";", 
    RowBox[{"ztail", "=", 
     RowBox[{"temp", "/.", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"x", "\[Rule]", 
         RowBox[{"pt", "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], ",", 
        RowBox[{"y", "\[Rule]", 
         RowBox[{"pt", "[", 
          RowBox[{"[", "2", "]"}], "]"}]}]}], "}"}]}]}], ";", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"ztip", "-", "ztail"}], ")"}], "/", 
     RowBox[{"Norm", "[", "dir", "]"}]}]}], ")"}]}]], "Input",
 CellChangeTimes->{{3.715386320202126*^9, 3.715386385542386*^9}, {
  3.7153866451594954`*^9, 3.715386674174397*^9}, {3.715386713586364*^9, 
