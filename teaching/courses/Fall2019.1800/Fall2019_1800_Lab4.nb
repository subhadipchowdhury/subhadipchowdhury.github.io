Cell[BoxData[
 RowBox[{
  RowBox[{"GradAngle", "[", 
   RowBox[{"func_", ",", "pt_", ",", "dir_"}], "]"}], ":=", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"g1", "=", 
     RowBox[{"Grad", "[", 
      RowBox[{"func", ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "y"}], "}"}]}], "]"}]}], ";", 
    RowBox[{"gradpt", "=", 
     RowBox[{"g1", "/.", 
      RowBox[{"{", 
