        RowBox[{"x", "\[Rule]", 
         RowBox[{"pt", "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], ",", 
        RowBox[{"y", "\[Rule]", 
         RowBox[{"pt", "[", 
          RowBox[{"[", "2", "]"}], "]"}]}]}], "}"}]}]}], ";", 
    RowBox[{"VectorAngle", "[", 
     RowBox[{"gradpt", ",", "dir"}], "]"}]}], ")"}]}]], "Input",
